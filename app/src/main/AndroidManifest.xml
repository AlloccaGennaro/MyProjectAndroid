<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gennaroallocca.myproject">

    <uses-permission android:name="android.permission.INTERNET"  />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-sdk android:minSdkVersion="9"
        android:targetSdkVersion="19"/>

    <application

        android:allowBackup="true"
        android:icon="@drawable/ima"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginActivity">
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.gennaroallocca.myproject.LOGINACTIVITY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".RegisterActivity"
            android:label="Glenocar Reister">
        </activity>


        <activity
            android:name=".ProfileActivity"
            android:label="Glenocar Profile">
        </activity>

        <activity
            android:name=".AboutActivity"
            android:label="Glenocar About">
        </activity>

        <activity
            android:name=".MusicActivity"
            android:label="Glenocar Profile">
        </activity>

    </application>

</manifest>
